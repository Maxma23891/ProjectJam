    textColor: {r: 0.6266194, g: 0.96981126, b: 0.43001065, a: 0.8392157}
    fontSize: 20
  - text: "\u0E08\u0E31\u0E21\u0E1B\u0E4C\u0E0A\u0E34\u0E1B\u0E2D\u0E22\u0E39\u0E48\u0E43\u0E19\u0E2A\u0E20\u0E32\u0E1E\u0E2A\u0E21\u0E1A\u0E39\u0E23\u0E13\u0E4C"
